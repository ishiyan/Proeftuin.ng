          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/ex01b",
      "sourceRoot": "projects/ex01b/src",
      "prefix": "ex01",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ex01b",
            "index": "projects/ex01b/src/index.html",
            "main": "projects/ex01b/src/main.ts",
            "polyfills": "projects/ex01b/src/polyfills.ts",
            "tsConfig": "projects/ex01b/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/ex01b/src/favicon.ico",
              "projects/ex01b/src/assets"
            ],
            "styles": [
              "projects/ex01b/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
